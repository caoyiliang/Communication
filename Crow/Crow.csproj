<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;net9.0;net8.0</TargetFrameworks>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Version>9.2.0</Version>
		<Description>数据收发队列</Description>
		<PackageId>CSoft.Crow</PackageId>
		<Authors>CSoft</Authors>
		<Company>CSoft</Company>
		<PackageReleaseNotes>优化任务管理，提高资源使用效率</PackageReleaseNotes>
		<PackageProjectUrl>https://github.com/caoyiliang/Communication</PackageProjectUrl>
		<RepositoryUrl>https://github.com/caoyiliang/Communication</RepositoryUrl>
		<PackageIcon>logo.png</PackageIcon>
		<ImplicitUsings>enable</ImplicitUsings>
		<LangVersion>default</LangVersion>
		<DocumentationFile>bin\Crow.xml</DocumentationFile>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\logo.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="CSoft.Utils" Version="9.1.4" />
		<PackageReference Include="System.Threading.Channels" Version="9.0.2" />
	</ItemGroup>

</Project>
